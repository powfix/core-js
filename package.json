{
  "name": "@powfix/core-js",
  "version": "0.26.6",
  "description": "core package",
  "author": "Kwon Kyung-Min <powfix@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/powfix/core-js"
  },
  "keywords": [
    "javascript",
    "core",
    "util"
  ],
  "homepage": "https://github.com/powfix/core-js",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist | wc -l",
    "esmfix": "tsc-esm-fix --ext='.js' dist/**/*.js",
    "compile": "tsc",
    "bundle": "tsup --config tsup.config.ts",
    "prebuild": "yarn clean",
    "build": "yarn compile && yarn bundle && yarn build:dts",
    "build:dts": "tsc -p tsconfig.declaration.json",
    "postbuild": "yarn esmfix"
  },
  "files": [
    "dist",
    "AUTHORS",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/index.browser.d.ts",
      "import": "./dist/index.browser.js",
      "require": "./dist/index.browser.cjs"
    },
    "./node": {
      "types": "./dist/index.node.d.ts",
      "import": "./dist/index.node.js",
      "require": "./dist/index.node.cjs"
    }
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "eventemitter3": "^5.0.1",
    "jwt-decode": "^4.0.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@powfix/uuid": "^0.2.17",
    "@swc/core": "1.15.3",
    "@types/base-64": "1.0.2",
    "@types/node": "24.10.2",
    "@types/uuid": "9.0.7",
    "axios": "^1.13.2",
    "deepmerge": "^4.3.1",
    "i18next": "25.7.2",
    "moment": "^2.30.1",
    "pg-hstore": "^2.3.4",
    "redis": "5.10.0",
    "sequelize": "6.37.7",
    "sequelize-typescript": "^2.1.6",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eventemitter3": ">=5",
    "moment": ">=2.0.0"
  }
}
